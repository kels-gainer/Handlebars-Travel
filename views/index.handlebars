
<div class="container">
	<div class="row">
		<div class="col-md-12 text-center" id="top-bar">
			{{!-- <img src="/assets/images/burger.png" alt="burger" /> --}}
			<h1> title! </h1>
		</div>
		<div class="col-md-6 text-center" class="task">
			<div class="row">
			  {{#each travels}}
			  	{{#unless this.visited}}
				<div class="col-md-9 text-center">
					<pre>{{this.id}} . {{this.place_name}}
					</pre>
				  		<!-- {{this.Date}} -->
				</div>
				<div class="col-md-3 text-center">
					<form class="devour-form button-size">
						<input type="hidden" class="" type="text" value={{this.id}}><br>
						<button type="submit" class="btn btn-default btn-add" data-id={{this.id}}>Go!</button>
					</form>
				</div>
				{{/unless}}
			  {{/each}}
			</div>
		</div>
		<div class="col-md-6 text-center" class="task">
			  {{#each travels}}
			  	{{#if this.visited}}
			  		<input class="form-control" type="text" placeholder="{{this.id}} . {{this.place_name}}" readonly>
				{{/if}}
			  {{/each}}
		</div>
	</div>
</div>

<div class="container">
	<div class="row">
		<div class="col-md-12 text-center enter_section">
			<form action="/api/places/" method="POST">
				<input type="text" class="form-control" name="place_name" value="" id="enter_text">
				<button type="submit" class="btn btn-primary" id="text-enter-button">Submit</button>
			</form>
			<p>Enter where you want to go then hit the "Submit" button.</p>
		</div>
	</div>
</div>




